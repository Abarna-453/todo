            String sql = "UPDATE Project SET uid = ?, role_id = ?, project_topic = ?, submission_date = ?, status_id = ?, mentor_id = ? WHERE project_id = ?";
            PreparedStatement pstmt = conn.prepareStatement(sql);
            pstmt.setInt(1, project.getUid());
