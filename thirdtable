                projects.add(new Project(rs.getInt("project_id"), rs.getInt("uid"), rs.getInt("role_id"),
                                         rs.getString("project_topic"), rs.getDate("submission_date").toLocalDate(), rs.getInt("status_id"), rs.getInt("mentor_id")));
            }
